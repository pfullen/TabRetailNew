@import "../library/variables";
$output-bourbon-deprecation-warnings: false;

html {
  background-color: $base-background-color;
  box-sizing: border-box;
}

*,
*::before,
*::after {
  box-sizing: inherit;
}

figure {
  margin: 0;
}

img,
picture {
  margin: 0;
  max-width: 100%;
}


.navigation__link {
	
	text-decoration: none;
}
