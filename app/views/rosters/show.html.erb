<div class = "jumbotron">

Week of: <%= @roster.week.strftime("%m/%d/%y") %> 

<table class = "table table-striped table-hover">
  <thead>
    <tr>
      
      <th>Employee</th>
      <th>Phone</th>
      <th> Per Diem </th>
      <th> Expense Code </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>



<% for list in @roster.roster_lists %>
<tr>
<td> <%= list.employee.full_name %> </td>
<td> <%= list.employee.phone %> </td>
<td> <%= list.perdiem%> </td>
<td> <%= list.expense_code %> </td>
</tr>
<% end %>
 </tbody>
</table>
<td><%= link_to 'Add Employee',    new_roster_roster_list_path(@roster) %></td>

<td><%= link_to 'Back to Project',    project_path(@roster.project) %></td>

				      
         </div>